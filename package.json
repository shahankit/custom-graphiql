{
  "name": "custom-graphiql",
  "version": "0.2.0",
  "description": "GraphiQL IDE with extra option for creating stub mutation and save queries to localStorage",
  "main": "dist/index.js",
  "scripts": {
    "build": ". ./resources/build.sh",
    "lint": "eslint src",
    "dev": "webpack-dev-server --config server/webpack.config.js --color --progress",
    "prepublish": ". ./resources/prepublish.sh",
    "preversion": ". ./resources/checkgit.sh",
    "update-website": ". ./resources/updateWebsite.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahankit/custom-graphiql.git"
  },
  "keywords": [
    "graphql",
    "graphiql",
    "react",
    "ide",
    "server",
    "custom"
  ],
  "author": "Ankit Shah (http://github.com/shahankit)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shahankit/custom-graphiql/issues"
  },
  "homepage": "https://github.com/shahankit/custom-graphiql#readme",
  "files": [
    "dist",
    "custom-graphiql.js",
    "custom-graphiql.min.js",
    "custom-graphiql.css",
    "README.md",
    "LICENSE"
  ],
  "browserify-shim": {
    "react": "global:React",
    "react-dom": "global:ReactDOM"
  },
  "dependencies": {
    "graphiql": "0.12.0",
    "isomorphic-fetch": "2.2.1",
    "react-copy-to-clipboard": "5.0.1"
  },
  "peerDependencies": {
    "graphql": "^0.6.0 || ^0.7.0 || ^0.8.0-b || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0",
    "prop-types": ">=15.5.0",
    "react": "^15.6.0 || ^16.0.0",
    "react-dom": "^15.6.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "browserify": "16.2.3",
    "browserify-shim": "3.8.14",
    "css-loader": "2.1.0",
    "eslint": "5.12.1",
    "eslint-config-prettier": "3.5.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-react": "7.12.4",
    "express": "4.16.4",
    "graphql": "0.13.2",
    "html-webpack-plugin": "3.2.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "remove-flow-types-loader": "1.1.0",
    "style-loader": "0.23.1",
    "uglify-js": "3.4.9",
    "uglifyify": "5.0.1",
    "webpack": "4.19.1",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  }
}
